require 'json'
require 'open-uri'

item do
  expected do
    static
    url = 'https://download.gnome.org/sources/gobject-introspection/cache.json'
    raw = JSON.parse(open(url).read)
    set raw[2]['gobject-introspection'].select { |x| x.split('.')[1].to_i.even? }.last
  end

  actual do
    homebrew_formula
    name 'gobject-introspection-halyard'
  end
end
